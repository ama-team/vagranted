{
  "name": "ama-team/vagranted",
  "description": "Vagrantfile manager",
  "minimum-stability": "stable",
  "license": "MIT",
  "version": "0.1.0",
  "authors": [
    {
      "name": "Etki",
      "email": "etki@etki.me"
    }
