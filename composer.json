{
  "name": "ama-team/vagranted",
  "description": "Vagrantfile manager",
  "minimum-stability": "stable",
  "license": "MIT",
  "version": "0.1.0-rc",
  "authors": [
    {
      "name": "Etki",
      "email": "etki@etki.me"
    }
  ],
  "autoload": {
    "psr-4": {
      "AmaTeam\\Vagranted\\": "src/"
    }
  },
  "require": {
    "php": ">= 5.6",
    "ext-json": "*",
    "twig/twig": "1.33.2",
    "symfony/console": ">=2.0 <4.0",
    "symfony/yaml": ">=2.0 <4.0",
    "symfony/process": ">=2.0 <4.0",
    "monolog/monolog": "^1.22",
    "symfony/dependency-injection": ">=2.0 <4.0",
    "symfony/config": ">=2.0 <4.0",
    "symfony/serializer": ">=2.0 <4.0",
    "symfony/property-access": ">=2.0 <4.0",
    "symfony/event-dispatcher": ">=2.0 <4.0",
    "kzykhys/git": "^0.1.2"
  },
  "require-dev": {
    "codeception/codeception": "^2.2",
    "php-vfs/php-vfs": "^1.3",
    "satooshi/php-coveralls": "^1.0"
  },
  "bin": ["bin/vagranted"],
  "scripts": {
    "test": "codecept build && codecept run --coverage --coverage-xml --coverage-html --xml",
    "test:report:publish": "composer test:report:publish:coverage",
    "test:report:publish:coverage": "coveralls"
  },
  "config": {
    "bin-dir": "bin"
  }
}
